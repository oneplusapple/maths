x^2yz
from sympy.vector import*
from sympy import symbols
N=CoordSys3D('')
x,y,z=symbols('x y z')
A=N.x**2*N.y*N.z
print("The given scalar point function is:")
display(A)
D=Del()
display(D(A))
w=gradient(A)
print("Gradient of F is:")
display(w)

3x^2yz
from sympy.vector import*
from sympy import symbols
N=CoordSys3D('')
x,y,z=symbols('x y z')
A=3*N.x**2*N.y*N.Z
print("The given scalar point function is:")
display(A)
D=Del()
display(D(A))
w=gradient(A)
print("Gradient of F is:")
display(w)

x^2y^3z^4
from sympy.vector import*
from sympy import symbols
N=CoordSys3D('')
x,y,z=symbols('x y z')
A=N.x**2*N.y**3*N.z**4
print("The given scalar point function is:")
display(A)
D=Del()
display(D(A))
w=gradient(A)
print("Gradient of F is:")
display(w)
